{
  "name": "new-typeorm-project",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "scripts": {
    "start": "nodemon --exec ts-node src/app.ts",
    "start:prod": "cross-env NODE_ENV=production npx pm2 start ./dist/app.js --name express",
    "stop": "npx pm2 stop express & npx pm2 delete express",
    "copyfiles": "cp -r ./src/entity/view/generation-view/query/ ./dist/entity/view/generation-view/query",
    "build": "npx tsc & npm run copyfiles"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^8.0.29",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.7",
    "pm2": "^4.5.1",
    "ts-node": "3.3.0",
    "typescript": "3.3.3333"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.29",
    "winston": "^3.3.3"
  },
